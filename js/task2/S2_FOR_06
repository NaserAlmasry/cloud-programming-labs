// S2_FOR_06 â€” Sum nested arrays

function sumNested(matrix) {
  let sum = 0;

  for (const row of matrix) {
    for (const n of row) {
      sum += n;
    }
  }

  return sum;
}

// tests
console.log(sumNested([[1, 2], [3, 4]]));        // 10
console.log(sumNested([[10], [20, 30], [40]])); // 100
console.log(sumNested([]));                     // 0
