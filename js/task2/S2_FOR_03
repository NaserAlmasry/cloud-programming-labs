// S2_FOR_03 â€” Sum until threshold

function sumUntil(nums, threshold) {
  let sum = 0;

  for (let n of nums) {
    if (sum + n > threshold) {
      break;
    }
    sum += n;
  }

  return sum;
}

// tests
console.log(sumUntil([1, 2, 3, 4], 6));   // 6  (1+2+3)
console.log(sumUntil([5, 5, 5], 9));      // 5
console.log(sumUntil([1, 1, 1], 10));     // 3
console.log(sumUntil([], 10));            // 0
