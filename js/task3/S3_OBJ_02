// S3_OBJ_02 â€” Merge config (shallow merge)
// Overrides win. Do not mutate inputs.

function mergeDefaults(defaults, overrides) {
  return {
    ...defaults,
    ...overrides,
  };
}

// tests
const defaults = { host: "localhost", port: 3000, debug: false };
const overrides = { port: 8080, debug: true };

const merged = mergeDefaults(defaults, overrides);

console.log(merged);    // { host: 'localhost', port: 8080, debug: true }
console.log(defaults);  // unchanged
console.log(overrides); // unchanged
