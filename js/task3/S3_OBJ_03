// S3_OBJ_03 â€” Pick selected keys from object

function pick(obj, keys) {
  const result = {};

  for (const key of keys) {
    if (key in obj) {
      result[key] = obj[key];
    }
  }

  return result;
}

// tests
const user = {
  id: 1,
  name: "Ala",
  email: "ala@example.com",
  role: "admin",
};

console.log(pick(user, ["id", "name"]));
// { id: 1, name: 'Ala' }

console.log(pick(user, ["email", "missing"]));
// { email: 'ala@example.com' }

console.log(pick(user, []));
// {}
